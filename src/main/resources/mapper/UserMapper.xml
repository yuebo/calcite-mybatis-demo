<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.calcite.UserMapper">
    <select id="findList" resultType="com.example.calcite.UserEntity">
        select a.* from db1.tbl_user a join db2.SYS_USER b on a.email = b.email
    </select>
</mapper>